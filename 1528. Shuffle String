char* restoreString(char* s, int* indices, int indicesSize) {
    
    int apoioIndices=0;
    char apoioString;

    for(int j=0;j < indicesSize; j++){
        for(int i=0; i < indicesSize; i++){
            if(indices[i] > indices[j]){
                apoioIndices = indices[i];
                indices[i] = indices[j];
                indices[j] = apoioIndices;

                apoioString = s[i];
                s[i] = s[j];
                s[j] = apoioString;   
            }
        }
    }
    return s;
}
